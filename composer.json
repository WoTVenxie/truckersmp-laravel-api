{
    "name": "wotvenxie/truckersmp-laravel-api",
    "description": "Laravel 12 client for the TruckersMP API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WoTVenxie\\TruckersMPApiClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "WoTVenxie",
            "email": "youremail@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WoTVenxie\\TruckersMPApiClient\\Providers\\TruckersMPServiceProvider"
            ],
            "aliases": {
                "TruckersMP": "WoTVenxie\\TruckersMPApiClient\\Facades\\TruckersMP"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
